{
    "name": "Potatoes Emojis",

    "version": "1.1",

    "description": "Replace latest noto-emoji from Google with old \"potatoes\"(or \"worms\" or whatever you call it...) emojis from Android 6.0.1 on the Google website Messages for web (or by a newer version if not available in 6.0.1)",

    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://messages.android.com/*",
        "*://ssl.gstatic.com/*"
    ],

    "background": {
        "scripts": ["replace-emojis.js"]
    },

    "icons": {
        "48": "icons/sleuth-or-spy_1f575_48.png",
        "96": "icons/sleuth-or-spy_1f575_96.png",
        "128": "icons/sleuth-or-spy_1f575_128.png"
    },

    "developer": {
        "name": "Matthieu Houdebine",
        "url": "https://github.com/mathoudebine/potatoes-emoji-webext"
    },
    
    "manifest_version": 2
}